{
  "version": 2,
  "builds": [
    {
      "src": "./**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": ".nxc/output"
      }
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.8"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.8"
    },
    "command": "cd .nxc && pip install -r requirements.txt && nxc build -i .. -o ./output --lunr --commits"
  }
}
