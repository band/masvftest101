{
  "version": 2,
  "framework": null,
  "build": "cd .nxc && nxc build -i .. -o ./output --lunr --commits",
  "outputDirectory": ".nxc/output",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/.nxc/output/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.8"
  },
  "installCommand": "pip install -r .nxc/requirements.txt",
  "public": true
}
