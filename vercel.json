{
  "version": 2,
  "buildCommand": "cd .nxc && npm install && python -m pip install --upgrade pip && pip install -r requirements.txt && nxc -i .. -o ../public --lunr --commits",
  "outputDirectory": "public",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/mwb-static/(.*)",
      "dest": "/mwb-static/$1"
    },
    {
      "src": "/(.+\\.html)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.8",
    "NODE_VERSION": "16"
  }
}
