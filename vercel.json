{
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.py": {
      "runtime": "vercel-python@3.8"
    }
  },
  "builds": [
    {
      "src": ".nxc/requirements.txt",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": ".nxc/nxc",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "pythonVersion": "3.8"
}
